- name: variable
  hosts: all
  vars:
    Url: play.google.com
  tasks:
    - name: url1
      debug:
        msg: "url = {{Url}}"
    - name: url2
      vars:
        Url: task.google.com
      debug:
        msg: "url = {{Url}}"

- name: invar
  hosts: all
  tasks:
    - name : url1
      debug:
        msg: "url = {{url}}"

- name: file
  hosts: all
  vars_files:
    - var.file
  tasks:
   - name: url2
     debug:
       msg: "url = {{url}}"


- hosts: all
  tasks:
        - name: fact
          debug:
            msg: "Hostname = {{ansible_fqdn}}"


- name: Demo on register variables
  hosts: all
  tasks:
    - name: Run uptime command
      ansible.builtin.shell: uptime
      register: out

       - name: Print output
            ansible.builtin.debug:
              msg: "{{out}}"

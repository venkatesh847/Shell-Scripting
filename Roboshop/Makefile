help: ##This is help message
	@fgrep -h "##" $(MAKEFILE_LIST) | fgrep -v fgrep | sed -e 's/\\$$//' | sed -e 's/##//'
frontend: ##Installing frontend
	@echo installing frontend
catalogue: ##Installing catalogue
	@echo installing catalouge
